RES6				VARCHAR2(5) DEFAULT '#', 
SIGUIENTE_SORTEO7   NUMBER DEFAULT 0,
PRED7				VARCHAR2(5) DEFAULT '#', 
PRES7				FLOAT DEFAULT 0.0, 
RES7				VARCHAR2(5) DEFAULT '#', 
SIGUIENTE_SORTEO8   NUMBER DEFAULT 0,
PRED8				VARCHAR2(5) DEFAULT '#', 
