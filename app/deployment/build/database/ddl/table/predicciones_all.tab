PRED4				VARCHAR2(5) DEFAULT '#', 
PRES4				FLOAT DEFAULT 0.0, 
RES4				VARCHAR2(5) DEFAULT '#', 
MATCH4				NUMBER(1) DEFAULT 0, 
SIGUIENTE_SORTEO5   NUMBER,
PRED5				VARCHAR2(5) DEFAULT '#',
PRES5				FLOAT DEFAULT 0.0, 
RES5				VARCHAR2(5) DEFAULT '#', 
MATCH5				NUMBER(1) DEFAULT 0, 
SIGUIENTE_SORTEO6   NUMBER,
