CREATE TABLE OLAP_SYS.S_GL_MAPAS_FRE_LT_CNT(
MASTER_ID		NUMBER NOT NULL,
GL_TYPE			VARCHAR2(5) NOT NULL,
SEQ_NO			NUMBER NOT NULL,
DRAWING_ID_INI	NUMBER NOT NULL,
B_TYPE_INI		VARCHAR2(2) NOT NULL,
GL_COLOR_INI	NUMBER DEFAULT 0,
GL_CNT_INI		NUMBER DEFAULT 0 NOT NULL,
GL_CNT_INI_RANK NUMBER DEFAULT 0,
PREDICTED_MANUAL_INI	VARCHAR2(2),
PREDICTED_FLAG_INI  	VARCHAR2(2),
WINNER_FLAG_INI			VARCHAR2(1), 
DRAWING_ID_END	NUMBER NOT NULL,
B_TYPE_END		VARCHAR2(2) NOT NULL,
GL_COLOR_END	NUMBER DEFAULT 0,
GL_CNT_END		NUMBER DEFAULT 0 NOT NULL,
GL_OUTPUT		VARCHAR2(1),
GL_CNT_END_RANK NUMBER DEFAULT 0,
PREDICTED_MANUAL_END 	VARCHAR2(2),
PREDICTED_FLAG_END  	VARCHAR2(2),
WINNER_FLAG_END			VARCHAR2(1), 
CREATED_BY		VARCHAR2(30) DEFAULT USER NOT NULL,
CREATION_DATE	DATE DEFAULT SYSDATE  NOT NULL) TABLESPACE OLAP_BIG_DATA_TBL
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.MASTER_ID IS 'Identificador principal derivado de la tabla S_GL_MAPAS'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.GL_TYPE IS 'FR: Frecuenia, LT: Ley Tercio'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.SEQ_NO IS 'Columna para ordenamiento de datos'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.DRAWING_ID_INI IS 'Valor derivado de la tabla SL_GAMBLINGS'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.B_TYPE_INI IS 'B1, B2, B3, B4, B5, B6'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.GL_COLOR_INI IS 'Color basado en GL'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.GL_CNT_INI IS 'Contador de coincidencias'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.DRAWING_ID_END IS 'Valor derivado de la tabla SL_GAMBLINGS'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.B_TYPE_END IS 'B1, B2, B3, B4, B5, B6'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.GL_COLOR_END IS 'Color basado en GL'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.GL_CNT_END IS 'Contador de coincidencias'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.CREATED_BY IS 'Usuario que creo el registro'
/
 COMMENT ON COLUMN S_GL_MAPAS_FRE_LT_CNT.CREATION_DATE IS 'Fecha de creacion del registro'
/
